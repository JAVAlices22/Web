import java.awt.*;
import javax.swing.*;

/**
 *
 * @author USUARIO
 */
public class TabuleiroRun extends JPanel{

    
    static Graphics g;
    public static void run(String[] args) {
        JFrame frame = new JFrame("WebChess");  
        frame.setDefaultCloseOperation(JFrame.DISPOSE_ON_CLOSE);  
        frame.add(new Tabuleiro());  
        frame.setSize(615, 640);  
        frame.validate();  
        frame.setLocationRelativeTo(null); 
        frame.setVisible(true);  
        
        
    }
    
}
